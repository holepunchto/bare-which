{
  "name": "bare-which",
  "version": "1.0.0",
  "description": "Finds instances of an executable in PATH",
  "main": "index.js",
  "files": [
    "index.js",
    "lib"
  ],
  "scripts": {
    "test": "npm run lint && npm run test:node && npm run test:bare",
    "test:node": "node test/all.js",
    "test:bare": "bare test/all.js",
    "lint": "standard",
    "lint:fix": "standard --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/holepunchto/bare-which.git"
  },
  "author": "Holepunch",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/holepunchto/bare-which/issues"
  },
  "homepage": "https://github.com/holepunchto/bare-which#readme",
  "imports": {
    "fs": {
      "bare": "bare-fs",
      "default": "fs"
    },
    "path": {
      "bare": "bare-path",
      "default": "path"
    },
    "process": {
      "bare": "bare-process",
      "default": "process"
    }
  },
  "dependencies": {
    "bare-fs": "^2.2.2",
    "bare-path": "^2.1.0",
    "bare-process": "^1.3.2",
    "which-runtime": "^1.2.0"
  },
  "devDependencies": {
    "brittle": "^3.5.0",
    "standard": "^17.1.0"
  }
}
